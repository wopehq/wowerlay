{
   "name": "wowerlay",
   "version": "0.0.10",
   "main": "./dist/wowerlay.umd.js",
   "module": "./dist/wowerlay.es.js",
   "types": "./dist/types/lib.d.ts",
   "exports": {
      ".": {
         "require": "./dist/wowerlay.umd.js",
         "import": "./dist/wowerlay.es.js"
      }
   },
   "repository": {
      "url": "git+https://github.com/teamseodo/wowerlay.git",
      "type": "git"
   },
   "bugs": {
      "url": "https://github.com/teamseodo/wowerlay/issues"
   },
   "homepage": "https://github.com/teamseodo/wowerlay#readme",
   "keywords": [
      "vue 3",
      "simplebar"
   ],
   "license": "MIT",
   "scripts": {
      "dev": "vite",
      "build": "ts-node -T --project tsconfig-build.json scripts/build.ts",
      "release": "ts-node -T --project tsconfig-build.json scripts/release.ts",
      "script:build": "tsc --project tsconfig-build.json && cross-env MODE=production vite build",
      "watch": "cross-env MODE=production vite build --watch"
   },
   "devDependencies": {
      "@types/fs-extra": "^9.0.13",
      "@types/node": "^16.10.3",
      "@types/prompts": "^2.0.14",
      "@vitejs/plugin-vue-jsx": "^1.2.0",
      "chalk": "^4.1.2",
      "cross-env": "^7.0.3",
      "execa": "^5.1.1",
      "fast-glob": "^3.2.7",
      "fs-extra": "^10.0.0",
      "prompts": "^2.4.2",
      "sass": "^1.42.1",
      "ts-node": "^10.2.1",
      "typescript": "4.3.5",
      "vite": "^2.6.4",
      "vite-plugin-dts": "^0.8.3",
      "vue": "3.2.20"
   },
   "peerDependencies": {
      "vue": "3.2.0"
   }
}